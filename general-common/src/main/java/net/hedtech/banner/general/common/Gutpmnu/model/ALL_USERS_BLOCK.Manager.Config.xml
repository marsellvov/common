<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="ALL_USERS_BLOCK.Manager.Config" name="AllUsersBlock">
		
		<items>
			<item name="USER_NAME" logicalName="userName" type="String"  virtual="true" />
			<item name="USER_ID" logicalName="userId" type="java.math.BigDecimal"  virtual="true" />
		</items>
		
		<adapterClass>net.hedtech.banner.general.common.Gutpmnu.model.AllUsersBlockAdapter</adapterClass>
		<queryAllRecords>true</queryAllRecords>
		<pagination maximumRows="18" enabled="true"/>
	</manager>
	
</b:beans>
