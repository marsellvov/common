<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="SHOW_HELP.Manager.Config" name="ShowHelp">
		<kind>SimpleBusinessObject</kind>
		<items>
			<item name="HELP_COMMENT" logicalName="helpComment" type="java.sql.Clob"   />
			<item name="HELP_IND" logicalName="helpInd" type="String"   />
			<item name="HELP_CHG" logicalName="helpChg" type="String"   >
				<value>
					<constant>N</constant>
				</value>	
			</item>
			<item name="HOLD_COMMENT" logicalName="holdComment" type="String"   />
		</items>
		
			<managerClass>net.hedtech.banner.general.common.Guahelp.model.ShowHelp</managerClass>

	</manager>
</b:beans>
