				<value>
					<constant>N</constant>
				</value>	
			</item>
			<item name="STVDEGC_SYSTEM_REQ_IND" logicalName="stvdegcSystemReqInd" type="String" physicalName="STVDEGC_SYSTEM_REQ_IND"  />
			<item name="STVDEGC_ACTIVITY_DATE" logicalName="stvdegcActivityDate" type="java.util.Date" physicalName="STVDEGC_ACTIVITY_DATE"  />
		</items>
		
		<adapterClass>net.hedtech.banner.general.common.Stvdegc.model.StvdegcAdapter</adapterClass>
		<defaultOrderByClause><![CDATA[stvdegc_code]]></defaultOrderByClause>	

