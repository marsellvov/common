<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="STVDEGC.Manager.Config" name="STVDEGC">
		
		<items>
			<item name="STVDEGC_CODE" logicalName="stvdegcCode" type="String" physicalName="STVDEGC_CODE"  />
			<item name="STVDEGC_DESC" logicalName="stvdegcDesc" type="String" physicalName="STVDEGC_DESC"  />
			<item name="STVDEGC_FA_COUNT_IND" logicalName="stvdegcFaCountInd" type="String" physicalName="STVDEGC_FA_COUNT_IND"  />
