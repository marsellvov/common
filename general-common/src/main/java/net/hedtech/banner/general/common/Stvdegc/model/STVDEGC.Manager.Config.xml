			<item name="STVDEGC_ACAT_CODE" logicalName="stvdegcAcatCode" type="String" physicalName="STVDEGC_ACAT_CODE"  />
			<item name="STVDEGC_VR_MSG_NO" logicalName="stvdegcVrMsgNo" type="java.math.BigDecimal" physicalName="STVDEGC_VR_MSG_NO"  />
