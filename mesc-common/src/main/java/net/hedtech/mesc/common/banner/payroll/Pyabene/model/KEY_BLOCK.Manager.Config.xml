<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="KEY_BLOCK.Manager.Config" name="KeyBlock">
		<kind>SimpleBusinessObject</kind>
		<items>
			<item name="PIDM" logicalName="pidm" type="java.math.BigDecimal"    /> 
			<item name="ID" logicalName="id" type="String"    /> 
			<item name="NAME" logicalName="nName" type="String"    />  
			<item name="EMST_RULE" logicalName="emstRule" type="String"    /> 
			<item name="BCAT_CODE" logicalName="bcatCode" type="String"    /> 
			<item name="BCAT_DESC" logicalName="bcatDesc" type="String"    /> 
		</items>
		
			<managerClass>net.hedtech.mesc.common.banner.payroll.Pyabene.model.KeyBlock</managerClass>

	</manager>
</b:beans>
