		<adapterClass>net.hedtech.general.common.libraries.Soqolib.model.SorlcurAdapter</adapterClass>
		<defaultWhereClause><![CDATA[WHERE ((nvl(:GLOBAL_VIEW_CURRENT, 'N') = 'Y' AND sovlcur_current_ind = 'Y' AND sovlcur_active_ind = 'Y') OR (nvl(:GLOBAL_VIEW_CURRENT, 'N') = 'N')) AND (:S___CURRICULA_MODULE_CODE IS NULL OR sovlcur_lmod_code = :S___CURRICULA_MODULE_CODE) AND (:S___CURRICULA_MODULE_KEY_SEQNO IS NULL OR :S___CURRICULA_MODULE_KEY_SEQNO = 99 OR sovlcur_key_seqno = :S___CURRICULA_MODULE_KEY_SEQNO) AND ((:S___CURRICULA_MODULE_TERM_CODE IS NOT NULL AND ((sovlcur_term_code = :S___CURRICULA_MODULE_TERM_CODE AND (sovlcur_lmod_code) IN (sb_curriculum_str.f_recruit, sb_curriculum_str.f_admissions)) OR (sovlcur_lmod_code = sb_curriculum_str.f_outcome) OR (sovlcur_lmod_code = sb_curriculum_str.f_learner AND (sovlcur_term_code < :S___CURRICULA_MODULE_END_TERM OR (sovlcur_term_code <= :S___CURRICULA_MODULE_END_TERM AND sovlcur_term_code = '999999'))))) OR :S___CURRICULA_MODULE_TERM_CODE IS NULL)]]></defaultWhereClause>	
		<defaultOrderByClause><![CDATA[order by decode(sovlcur_lmod_code, sb_curriculum_str.f_recruit,'1',
   sb_curriculum_str.f_admissions,'2', 
   sb_curriculum_str.f_learner, '3', 
   sb_curriculum_str.f_outcome,'4', '5'), 
 decode(sovlcur_lmod_code, sb_curriculum_str.f_learner,'999999',
    sovlcur_lmod_code, sb_curriculum_str.f_outcome,'999999',     sovlcur_term_code),
 decode(sovlcur_lmod_code, sb_curriculum_str.f_learner,99, sovlcur_key_seqno), 
 decode(sovlcur_current_ind || sovlcur_active_ind,'YY',1,2),
 sovlcur_priority_no, sovlcur_seqno desc    ]]></defaultOrderByClause>	
		<queryAllRecords>true</queryAllRecords>	

