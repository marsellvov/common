<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="RQPO_SUSPENSE_BLOCK.Manager.Config" name="RqpoSuspenseBlock">
		
		<items>
			<item name="RECORD_COUNT" logicalName="recordCount" type="Integer"  virtual="true" >
				<value>
					<constant>0</constant>
				</value>	
			</item>
			<item name="SINGLE_ACCTG_IND" logicalName="singleAcctgInd" type="String"  virtual="true" />
			<item name="ACCTG_SUSP_IND" logicalName="acctgSuspInd" type="String"  virtual="true" />
			<item name="ACCTG_NSF_SUSP_IND" logicalName="acctgNsfSuspInd" type="String"  virtual="true" />
			<item name="COMM_SUSP_IND" logicalName="commSuspInd" type="String"  virtual="true" />
			<item name="ACCTG_SEQ_NO" logicalName="acctgSeqNo" type="java.math.BigDecimal"  virtual="true" />
			<item name="ITEM_NO" logicalName="itemNo" type="java.math.BigDecimal"  virtual="true" />
		</items>
		
		<adapterClass>net.hedtech.banner.finance.common.Foqolib.model.RqpoSuspenseBlockAdapter</adapterClass>

	</manager>
</b:beans>
